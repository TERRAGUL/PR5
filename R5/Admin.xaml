<Window x:Class="R5.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:R5"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800" Background="PeachPuff">
    <Grid>
        <ComboBox x:Name="CBXTABL" HorizontalAlignment="Left" Margin="669,0,0,0" VerticalAlignment="Top" Width="130" Height="28" SelectionChanged="CBXTABL_SelectionChanged"/>

        <Button x:Name="OpenTabl" Content="Открыть таблицу" HorizontalAlignment="Left" Margin="511,0,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.944,-0.316" Height="30" Width="153" Click="OpenTabl_Click"/>

        <Button x:Name="CreateUs" Content="Создать Сотрудни" HorizontalAlignment="Left" Margin="511,46,0,0" VerticalAlignment="Top" Width="153" Click="CreateUseraClick"/>
        <Button Content="Вернуться к авторизации" HorizontalAlignment="Left" Margin="0,381,0,0" VerticalAlignment="Top" Click="Exit_Button"/>


    </Grid>
</Window>
